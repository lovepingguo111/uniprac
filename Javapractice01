
import java.util.Arrays;

public class TestBubble {
	public static void main(String[] args) {
		int a[] = {3,2,5,88,32,12,1};
		int temp = 0;
		
		for(int i = 0;i < a.length;i++){
			boolean f = true;
		for(int j = 0;j < a.length -1;j++) {
			if(a[j] > a[j+1]) {
				temp = a[j];
				a[j] = a[j+1];
				a[j+1] = temp;	
				f = false;
			}
		}
			System.out.println(Arrays.toString(a));
			System.out.println("##################");
		}
		
	}

}
